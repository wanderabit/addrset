{
  "name": "addrset",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "download:rir": "tsx ./src/download-rir.ts",
    "download:bt-tracker": "tsx ./src/download-bt-tracker.ts",
    "download": "npm-run-all -lp download:*",
    "build:ip": "tsx ./src/build-ip.ts",
    "build:ip:go": "go run ./src/build-ip.go",
    "build:site": "tsx ./src/build-site.ts",
    "build": "npm-run-all -ls build:ip build:ip:go build:site",
    "clean": "rimraf ./dist/ip ./dist/site ./dist/clash",
    "clean:all": "rimraf ./dist"
  },
  "dependencies": {
    "cidr-tools": "^11.0.3",
    "ky": "^1.14.0",
    "maxmind": "^5.0.1",
    "psl": "^1.15.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
